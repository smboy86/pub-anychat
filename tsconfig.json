{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "~/*": ["@/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@navigations/*": ["navigations/*"],
      "@screens/*": ["screens/*"],
      "@assets/*": ["assets/*"],
      "@type/*": ["type/*"],
      "@utils/*": ["utils/*"]
    },
  }  
}
